model isotopes {
  id                      Int      @id @default(0)
  atomic_number_unique    Int?
  known_isotopes          String?  @db.Text
  stable_isotopes         String?  @db.Text
  is_monoisotopic         Boolean?
  atomic_number_notunique Int?
  mass                    Float?
  abundance               Float?
  mass_number             Int?
